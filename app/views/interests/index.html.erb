
<h1> Interests </h1>
	<table border=2>
		<thead>
			<tr>
				<th>Type</th>
				<th>Interest Name</th>
				<th colspan="3"></th>
			</tr>
		</thead>
		<tbody>
			<% @interests.each do |interest| %>
			<tr>
				<td><%= interest.type %></td>
				<td><%= interest.interest_name %> </td>
				<td><%= link_to "Show", interest %></td>
				<td><%= link_to "Edit", edit_interest_path(interest) %></td>
				<td><%= link_to "delete", interest_path(interest), method: :delete %></td>
			</tr>
			<% end %>
		</tbody>
	</table>

	<%= link_to "New Interest", new_interest_path %>